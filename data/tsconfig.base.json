{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ES6",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "importsNotUsedAsValues": "preserve",
    "noImplicitAny": true,
    "sourceMap": false,
    "esModuleInterop": true,
    "lib": ["DOM", "ES6", "ES7", "ES2018", "ES2019",
      "ES2020", "ESNext", "DOM.Iterable", "ScriptHost"]
  }
}
